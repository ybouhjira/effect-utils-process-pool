{
  "name": "@effect-utils/process-pool",
  "version": "0.0.1",
  "description": "Effect-based process pool with lifecycle management, concurrency limits, and orphan cleanup",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ tests/",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "effect",
    "process",
    "pool",
    "child-process",
    "lifecycle",
    "concurrency"
  ],
  "author": "Youssef Bouhjira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ybouhjira/effect-utils-process-pool"
  },
  "peerDependencies": {
    "@effect/platform": "^0.90.0",
    "@effect/platform-node": "^0.70.0",
    "effect": "^3.0.0"
  },
  "devDependencies": {
    "@effect/platform": "^0.92.0",
    "@effect/platform-node": "^0.73.1",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^3.0.6",
    "effect": "^3.16.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.6"
  }
}
